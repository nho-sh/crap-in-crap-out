{
  "name": "crap-in-crap-out",
  "version": "1.0.0",
  "description": "A strict JSON validator with a very simple, wysiwyg, human friendly schema",
  "main": "src/index.js",
  "scripts": {
    "pre-deploy": "npm run test && npm run lint",
    "debug": "mocha --nodejs --inspect-brk ./test/_entry.js",
    "package": "rm *.tgz && npm pack",
    "test": "nyc --lines=99 --check-coverage=true --reporter=html --reporter=text mocha ./test/_entry.js",
    "lint": "eslint 'src/*.js' -c ./.eslintrc"
  },
  "keywords": [
    "arguments",
    "function",
    "schema",
    "validation",
    "validator",
    "wysiwyg"
  ],
  "author": "nhosh",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "eslint": "^8.2.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0"
  }
}
